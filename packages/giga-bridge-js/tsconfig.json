/* Based on https://github.com/tsconfig/bases/blob/501da2bcd640cf95c95805783e1012b992338f28/bases/node22.json */
{
    "compilerOptions": {
      "target": "es2022",
      "module": "Node18",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": false,
      "skipLibCheck": true,
      "resolveJsonModule": true
    }
  }
  
